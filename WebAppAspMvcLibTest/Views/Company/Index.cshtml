@{
    ViewData["Title"] = "Companies";
}

<div class="text-center">
    
<p> HELLO Company</p>
    <table class="table">
        <tr><th>ID</th><th>Title</th><th>Type</th></tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.GetType().ToString()</td>

            </tr>
        }
    </table>
   

</div>
